{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@iconos/*": ["./public/Icons/*"],
      "@img/*": ["./public/img/*"],
      "@iconosFrame/*": ["./public/frameworks/*"],
      "@globals/*": ["./src/Globals/*"]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
